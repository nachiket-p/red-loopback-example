{
  "initial:before": {
    "loopback#favicon": {}
  },
  "initial": {
    "compression": {},
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    },
    "helmet#xssFilter": {},
    "helmet#frameguard": {
      "params": [
        "deny"
      ]
    },
    "helmet#hsts": {
      "params": {
        "maxAge": 0,
        "includeSubdomains": true
      }
    },
    "helmet#hidePoweredBy": {},
    "helmet#ieNoOpen": {},
    "helmet#noSniff": {},
    "helmet#noCache": {
      "enabled": false
    }
  },
  "session:before": {
    "cookie-parser": {
      "params": "TEMP_SEC"
    }
  },
  "session": {
    "express-session": {
      "params": {
        "secret": "TEMP_SEC",
        "saveUninitialized": true,
        "resave": true
      }
    }
  },
  "auth": {
    "loopback#token": {},
    "red-loopback/secure#red": {
      "params": {
        "paths": ["/red", "/red-api"],
        "roleModel": "Role",
        "roles": ["ADMIN" ]
      }
    }
  },
  "parse": {},
  "routes": {
    "loopback#rest": {
      "paths": [
        "${restApiRoot}"
      ]
    }
  },
  "files": {
    "loopback#static": {
      "params": "$!../client"
    }
  },
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "strong-error-handler": {}
  }
}
